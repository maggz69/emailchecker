{
    "name": "maggz69/emailchecker",
    "description": "An email mailbox checker and verifier for laravel",
    "license": "MIT",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "~6.5.5",
        "php":">=7.0"
    },
    "autoload": {
        "psr-4": {
            "EmailChecker\\": "src",
            "EmailChecker\\Pipeline\\":"src\\Pipeline",
            "EmailChecker\\Commands\\": "src\\Console"
        }
    },
    "authors": [
        {
            "name": "Pius Gumo",
            "email": "swagzmania@gmail.com"
        }
    ],
    "extra":{
        "laravel":{
            "providers":[
                "EmailChecker\\EmailCheckerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
